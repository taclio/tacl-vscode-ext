{
	"name": "TACL",
	"scopeName": "source.tacl",
	"patterns": [
		{
			"include": "#comment"
		},
		{
			"include": "#property"
		},
		{
			"include": "#directive"
		},
		{
			"include": "#keywords"
		},
		{
			"match": "^---",
			"name": "entity.other.document.begin.tacl"
		},
		{
			"match": "^\\.{3}",
			"name": "entity.other.document.end.tacl"
		},
		{
			"include": "#node"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
						"name": "keyword.control.tacl",
						"match": "\\b(on|of|not|available|checked|contains|editable|empty|enabled|hidden|title|visible)\\b"
					},
					{
						"name": "entity.name.type",
						"match": "\\b(Given|Then|And|variables|datasets)\\b"
					},
					{
						"name": "entity.name.function",
						"match": "\\b(check|click|double_click|fill|focus|mouse_move|open|press|right_click|screenshot|select|type|uncheck|assert)\\b"
					},
					{
						"name": "storage.type.function.ts",
						"match": "\\b(name|description|scenarios|steps|given|then|when|and|condition|value|do|actions|options|continueOnFailure|variables|datasets|locator|timeout|data|variable)\\b"
					},
					{
						"name": "variable.other.constant",
						"match": "(\\$\\(.*\\))"
					}
					
		]
		},
		"block-collection": {
			"patterns": [
				{
					"include": "#block-sequence"
				},
				{
					"include": "#block-mapping"
				}
			]
		},
		"block-mapping": {
			"patterns": [
				{
					"include": "#block-pair"
				}
			]
		},
		"block-node": {
			"patterns": [
				{
					"include": "#prototype"
				},
				{
					"include": "#block-scalar"
				},
				{
					"include": "#block-collection"
				},
				{
					"include": "#flow-scalar-plain-out"
				},
				{
					"include": "#flow-node"
				}
			]
		},
		"block-pair": {
			"patterns": [
				{
					"begin": "\\?",
					"beginCaptures": {
						"1": {
							"name": "punctuation.definition.key-value.begin.tacl"
						}
					},
					"end": "(?=\\?)|^ *(:)|(:)",
					"endCaptures": {
						"1": {
							"name": "punctuation.separator.key-value.mapping.tacl"
						},
						"2": {
							"name": "invalid.illegal.expected-newline.tacl"
						}
					},
					"name": "meta.block-mapping.tacl",
					"patterns": [
						{
							"include": "#block-node"
						}
					]
				},
				{
					"begin": "(?x)\n                        (?=\n                            (?x:\n                                  [^\\s[-?:,\\[\\]{}#&*!|>'\"%@`]]\n                                | [?:-] \\S\n                            )\n                            (\n                                  [^\\s:]\n                                | : \\S\n                                | \\s+ (?![#\\s])\n                            )*\n                            \\s*\n                            :\n\t\t\t\t\t\t\t(\\s|$)\n                        )\n                    ",
					"end": "(?x)\n                        (?=\n                              \\s* $\n                            | \\s+ \\#\n                            | \\s* : (\\s|$)\n                        )\n                    ",
					"patterns": [
						{
							"include": "#flow-scalar-plain-out-implicit-type"
						},
						{
							"begin": "(?x)\n                                  [^\\s[-?:,\\[\\]{}#&*!|>'\"%@`]]\n                                | [?:-] \\S\n                            ",
							"beginCaptures": {
								"0": {
									"name": "entity.name.tag.tacl"
								}
							},
							"contentName": "entity.name.tag.tacl",
							"end": "(?x)\n                                (?=\n                                      \\s* $\n                                    | \\s+ \\#\n                                    | \\s* : (\\s|$)\n                                )\n                            ",
							"name": "string.unquoted.plain.out.tacl"
						}
					]
				},
				{
					"match": ":(?=\\s|$)",
					"name": "punctuation.separator.key-value.mapping.tacl"
				}
			]
		},
		"block-scalar": {
			"begin": "(?:(\\|)|(>))([1-9])?([-+])?(.*\\n?)",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.flow.block-scalar.literal.tacl"
				},
				"2": {
					"name": "keyword.control.flow.block-scalar.folded.tacl"
				},
				"3": {
					"name": "constant.numeric.indentation-indicator.tacl"
				},
				"4": {
					"name": "storage.modifier.chomping-indicator.tacl"
				},
				"5": {
					"patterns": [
						{
							"include": "#comment"
						},
						{
							"match": ".+",
							"name": "invalid.illegal.expected-comment-or-newline.tacl"
						}
					]
				}
			},
			"end": "^(?=\\S)|(?!\\G)",
			"patterns": [
				{
					"begin": "^([ ]+)(?! )",
					"end": "^(?!\\1|\\s*$)",
					"name": "string.unquoted.block.tacl"
				}
			]
		},
		"block-sequence": {
			"match": "(-)(?!\\S)",
			"name": "punctuation.definition.block.sequence.item.tacl"
		},
		"comment": {
			"begin": "(?:(^[ \\t]*)|[ \\t]+)(?=#\\p{Print}*$)",
			"beginCaptures": {
				"1": {
					"name": "punctuation.whitespace.comment.leading.tacl"
				}
			},
			"end": "(?!\\G)",
			"patterns": [
				{
					"begin": "#",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.comment.tacl"
						}
					},
					"end": "\\n",
					"name": "comment.line.number-sign.tacl"
				}
			]
		},
		"directive": {
			"begin": "^%",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.directive.begin.tacl"
				}
			},
			"end": "(?=$|[ \\t]+($|#))",
			"name": "meta.directive.tacl",
			"patterns": [
				{
					"captures": {
						"1": {
							"name": "keyword.other.directive.tacl.tacl"
						},
						"2": {
							"name": "constant.numeric.tacl-version.tacl"
						}
					},
					"match": "\\G(tacl)[ \\t]+(\\d+\\.\\d+)"
				},
				{
					"captures": {
						"1": {
							"name": "keyword.other.directive.tag.tacl"
						},
						"2": {
							"name": "storage.type.tag-handle.tacl"
						},
						"3": {
							"name": "support.type.tag-prefix.tacl"
						}
					},
					"match": "(?x)\n                        \\G\n                        (TAG)\n                        (?:[ \\t]+\n                            ((?:!(?:[0-9A-Za-z\\-]*!)?))\n                            (?:[ \\t]+ (\n                                  !              (?x: %[0-9A-Fa-f]{2} | [0-9A-Za-z\\-#;/?:@&=+$,_.!~*'()\\[\\]] )*\n                                | (?![,!\\[\\]{}]) (?x: %[0-9A-Fa-f]{2} | [0-9A-Za-z\\-#;/?:@&=+$,_.!~*'()\\[\\]] )+\n                                )\n                            )?\n                        )?\n                    "
				},
				{
					"captures": {
						"1": {
							"name": "support.other.directive.reserved.tacl"
						},
						"2": {
							"name": "string.unquoted.directive-name.tacl"
						},
						"3": {
							"name": "string.unquoted.directive-parameter.tacl"
						}
					},
					"match": "(?x) \\G (\\w+) (?:[ \\t]+ (\\w+) (?:[ \\t]+ (\\w+))? )?"
				},
				{
					"match": "\\S+",
					"name": "invalid.illegal.unrecognized.tacl"
				}
			]
		},
		"flow-alias": {
			"captures": {
				"1": {
					"name": "keyword.control.flow.alias.tacl"
				},
				"2": {
					"name": "punctuation.definition.alias.tacl"
				},
				"3": {
					"name": "variable.other.alias.tacl"
				},
				"4": {
					"name": "invalid.illegal.character.anchor.tacl"
				}
			},
			"match": "((\\*))([^\\s\\[\\]/{/},]+)([^\\s\\]},]\\S*)?"
		},
		"flow-collection": {
			"patterns": [
				{
					"include": "#flow-sequence"
				},
				{
					"include": "#flow-mapping"
				}
			]
		},
		"flow-mapping": {
			"begin": "\\{",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.mapping.begin.tacl"
				}
			},
			"end": "\\}",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.mapping.end.tacl"
				}
			},
			"name": "meta.flow-mapping.tacl",
			"patterns": [
				{
					"include": "#prototype"
				},
				{
					"match": ",",
					"name": "punctuation.separator.mapping.tacl"
				},
				{
					"include": "#flow-pair"
				}
			]
		},
		"flow-node": {
			"patterns": [
				{
					"include": "#prototype"
				},
				{
					"include": "#flow-alias"
				},
				{
					"include": "#flow-collection"
				},
				{
					"include": "#flow-scalar"
				}
			]
		},
		"flow-pair": {
			"patterns": [
				{
					"begin": "\\?",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.key-value.begin.tacl"
						}
					},
					"end": "(?=[},\\]])",
					"name": "meta.flow-pair.explicit.tacl",
					"patterns": [
						{
							"include": "#prototype"
						},
						{
							"include": "#flow-pair"
						},
						{
							"include": "#flow-node"
						},
						{
							"begin": ":(?=\\s|$|[\\[\\]{},])",
							"beginCaptures": {
								"0": {
									"name": "punctuation.separator.key-value.mapping.tacl"
								}
							},
							"end": "(?=[},\\]])",
							"patterns": [
								{
									"include": "#flow-value"
								}
							]
						}
					]
				},
				{
					"begin": "(?x)\n                        (?=\n                            (?:\n                                [^\\s[-?:,\\[\\]{}#&*!|>'\"%@`]]\n                              | [?:-] [^\\s[\\[\\]{},]]\n                            )\n                            (\n                                  [^\\s:[\\[\\]{},]]\n                                | : [^\\s[\\[\\]{},]]\n                                | \\s+ (?![#\\s])\n                            )*\n                            \\s*\n                            :\n\t\t\t\t\t\t\t(\\s|$)\n                        )\n                    ",
					"end": "(?x)\n                        (?=\n                              \\s* $\n                            | \\s+ \\#\n                            | \\s* : (\\s|$)\n                            | \\s* : [\\[\\]{},]\n                            | \\s* [\\[\\]{},]\n                        )\n                    ",
					"name": "meta.flow-pair.key.tacl",
					"patterns": [
						{
							"include": "#flow-scalar-plain-in-implicit-type"
						},
						{
							"begin": "(?x)\n                                  [^\\s[-?:,\\[\\]{}#&*!|>'\"%@`]]\n                                | [?:-] [^\\s[\\[\\]{},]]\n                            ",
							"beginCaptures": {
								"0": {
									"name": "entity.name.tag.tacl"
								}
							},
							"contentName": "entity.name.tag.tacl",
							"end": "(?x)\n                                (?=\n                                      \\s* $\n                                    | \\s+ \\#\n                                    | \\s* : (\\s|$)\n                                    | \\s* : [\\[\\]{},]\n                                    | \\s* [\\[\\]{},]\n                                )\n                            ",
							"name": "string.unquoted.plain.in.tacl"
						}
					]
				},
				{
					"include": "#flow-node"
				},
				{
					"begin": ":(?=\\s|$|[\\[\\]{},])",
					"captures": {
						"0": {
							"name": "punctuation.separator.key-value.mapping.tacl"
						}
					},
					"end": "(?=[},\\]])",
					"name": "meta.flow-pair.tacl",
					"patterns": [
						{
							"include": "#flow-value"
						}
					]
				}
			]
		},
		"flow-scalar": {
			"patterns": [
				{
					"include": "#flow-scalar-double-quoted"
				},
				{
					"include": "#flow-scalar-single-quoted"
				},
				{
					"include": "#flow-scalar-plain-in"
				}
			]
		},
		"flow-scalar-double-quoted": {
			"begin": "\"",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.string.begin.tacl"
				}
			},
			"end": "\"",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.string.end.tacl"
				}
			},
			"name": "string.quoted.double.tacl",
			"patterns": [
				{
					"match": "\\\\([0abtnvfre \"/\\\\N_Lp]|x\\d\\d|u\\d{4}|U\\d{8})",
					"name": "constant.character.escape.tacl"
				},
				{
					"match": "\\\\\\n",
					"name": "constant.character.escape.double-quoted.newline.tacl"
				}
			]
		},
		"flow-scalar-plain-in": {
			"patterns": [
				{
					"include": "#flow-scalar-plain-in-implicit-type"
				},
				{
					"begin": "(?x)\n                          [^\\s[-?:,\\[\\]{}#&*!|>'\"%@`]]\n                        | [?:-] [^\\s[\\[\\]{},]]\n                    ",
					"end": "(?x)\n                        (?=\n                              \\s* $\n                            | \\s+ \\#\n                            | \\s* : (\\s|$)\n                            | \\s* : [\\[\\]{},]\n                            | \\s* [\\[\\]{},]\n                        )\n                    ",
					"name": "string.unquoted.plain.in.tacl"
				}
			]
		},
		"flow-scalar-plain-in-implicit-type": {
			"patterns": [
				{
					"captures": {
						"1": {
							"name": "constant.language.null.tacl"
						},
						"2": {
							"name": "constant.language.boolean.tacl"
						},
						"3": {
							"name": "constant.numeric.integer.tacl"
						},
						"4": {
							"name": "constant.numeric.float.tacl"
						},
						"5": {
							"name": "constant.other.timestamp.tacl"
						},
						"6": {
							"name": "constant.language.value.tacl"
						},
						"7": {
							"name": "constant.language.merge.tacl"
						}
					},
					"match": "(?x)\n                        (?x:\n                              (null|Null|NULL|~)\n                            | (y|Y|yes|Yes|YES|n|N|no|No|NO|true|True|TRUE|false|False|FALSE|on|On|ON|off|Off|OFF)\n                            | (\n                                (?:\n                                      [-+]? 0b [0-1_]+ # (base 2)\n                                    | [-+]? 0  [0-7_]+ # (base 8)\n                                    | [-+]? (?: 0|[1-9][0-9_]*) # (base 10)\n                                    | [-+]? 0x [0-9a-fA-F_]+ # (base 16)\n                                    | [-+]? [1-9] [0-9_]* (?: :[0-5]?[0-9])+ # (base 60)\n                                )\n                              )\n                            | (\n                                (?x:\n                                      [-+]? (?: [0-9] [0-9_]*)? \\. [0-9.]* (?: [eE] [-+] [0-9]+)? # (base 10)\n                                    | [-+]? [0-9] [0-9_]* (?: :[0-5]?[0-9])+ \\. [0-9_]* # (base 60)\n                                    | [-+]? \\. (?: inf|Inf|INF) # (infinity)\n                                    |       \\. (?: nan|NaN|NAN) # (not a number)\n                                )\n                              )\n                            | (\n                                (?x:\n                                    \\d{4} - \\d{2} - \\d{2}           # (y-m-d)\n                                  | \\d{4}                           # (year)\n                                    - \\d{1,2}                       # (month)\n                                    - \\d{1,2}                       # (day)\n                                    (?: [Tt] | [ \\t]+) \\d{1,2}      # (hour)\n                                    : \\d{2}                         # (minute)\n                                    : \\d{2}                         # (second)\n                                    (?: \\.\\d*)?                     # (fraction)\n                                    (?:\n                                          (?:[ \\t]*) Z\n                                        | [-+] \\d{1,2} (?: :\\d{1,2})?\n                                    )?                              # (time zone)\n                                )\n                              )\n                            | (=)\n                            | (<<)\n                        )\n                        (?:\n                            (?=\n                                  \\s* $\n                                | \\s+ \\#\n                                | \\s* : (\\s|$)\n                                | \\s* : [\\[\\]{},]\n                                | \\s* [\\[\\]{},]\n                            )\n                        )\n                    "
				}
			]
		},
		"flow-scalar-plain-out": {
			"patterns": [
				{
					"include": "#flow-scalar-plain-out-implicit-type"
				},
				{
					"begin": "(?x)\n                          [^\\s[-?:,\\[\\]{}#&*!|>'\"%@`]]\n                        | [?:-] \\S\n                    ",
					"end": "(?x)\n                        (?=\n                              \\s* $\n                            | \\s+ \\#\n                            | \\s* : (\\s|$)\n                        )\n                    ",
					"name": "string.unquoted.plain.out.tacl"
				}
			]
		},
		"flow-scalar-plain-out-implicit-type": {
			"patterns": [
				{
					"captures": {
						"1": {
							"name": "constant.language.null.tacl"
						},
						"2": {
							"name": "constant.language.boolean.tacl"
						},
						"3": {
							"name": "constant.numeric.integer.tacl"
						},
						"4": {
							"name": "constant.numeric.float.tacl"
						},
						"5": {
							"name": "constant.other.timestamp.tacl"
						},
						"6": {
							"name": "constant.language.value.tacl"
						},
						"7": {
							"name": "constant.language.merge.tacl"
						}
					},
					"match": "(?x)\n                        (?x:\n                              (null|Null|NULL|~)\n                            | (y|Y|yes|Yes|YES|n|N|no|No|NO|true|True|TRUE|false|False|FALSE|on|On|ON|off|Off|OFF)\n                            | (\n                                (?:\n                                      [-+]? 0b [0-1_]+ # (base 2)\n                                    | [-+]? 0  [0-7_]+ # (base 8)\n                                    | [-+]? (?: 0|[1-9][0-9_]*) # (base 10)\n                                    | [-+]? 0x [0-9a-fA-F_]+ # (base 16)\n                                    | [-+]? [1-9] [0-9_]* (?: :[0-5]?[0-9])+ # (base 60)\n                                )\n                              )\n                            | (\n                                (?x:\n                                      [-+]? (?: [0-9] [0-9_]*)? \\. [0-9.]* (?: [eE] [-+] [0-9]+)? # (base 10)\n                                    | [-+]? [0-9] [0-9_]* (?: :[0-5]?[0-9])+ \\. [0-9_]* # (base 60)\n                                    | [-+]? \\. (?: inf|Inf|INF) # (infinity)\n                                    |       \\. (?: nan|NaN|NAN) # (not a number)\n                                )\n                              )\n                            | (\n                                (?x:\n                                    \\d{4} - \\d{2} - \\d{2}           # (y-m-d)\n                                  | \\d{4}                           # (year)\n                                    - \\d{1,2}                       # (month)\n                                    - \\d{1,2}                       # (day)\n                                    (?: [Tt] | [ \\t]+) \\d{1,2}      # (hour)\n                                    : \\d{2}                         # (minute)\n                                    : \\d{2}                         # (second)\n                                    (?: \\.\\d*)?                     # (fraction)\n                                    (?:\n                                          (?:[ \\t]*) Z\n                                        | [-+] \\d{1,2} (?: :\\d{1,2})?\n                                    )?                              # (time zone)\n                                )\n                              )\n                            | (=)\n                            | (<<)\n                        )\n                        (?x:\n                            (?=\n                                  \\s* $\n                                | \\s+ \\#\n                                | \\s* : (\\s|$)\n                            )\n                        )\n                    "
				}
			]
		},
		"flow-scalar-single-quoted": {
			"begin": "'",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.string.begin.tacl"
				}
			},
			"end": "'(?!')",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.string.end.tacl"
				}
			},
			"name": "string.quoted.single.tacl",
			"patterns": [
				{
					"match": "''",
					"name": "constant.character.escape.single-quoted.tacl"
				}
			]
		},
		"flow-sequence": {
			"begin": "\\[",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.sequence.begin.tacl"
				}
			},
			"end": "\\]",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.sequence.end.tacl"
				}
			},
			"name": "meta.flow-sequence.tacl",
			"patterns": [
				{
					"include": "#prototype"
				},
				{
					"match": ",",
					"name": "punctuation.separator.sequence.tacl"
				},
				{
					"include": "#flow-pair"
				},
				{
					"include": "#flow-node"
				}
			]
		},
		"flow-value": {
			"patterns": [
				{
					"begin": "\\G(?![},\\]])",
					"end": "(?=[},\\]])",
					"name": "meta.flow-pair.value.tacl",
					"patterns": [
						{
							"include": "#flow-node"
						}
					]
				}
			]
		},
		"node": {
			"patterns": [
				{
					"include": "#block-node"
				}
			]
		},
		"property": {
			"begin": "(?=!|&)",
			"end": "(?!\\G)",
			"name": "meta.property.tacl",
			"patterns": [
				{
					"captures": {
						"1": {
							"name": "keyword.control.property.anchor.tacl"
